{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "quotes": [2, "single", { "avoidEscape": true }],
    "prettier/prettier": [
      "error", {
        "semi": true,
        "singleQuote": true,
			}
    ]
  }
}
